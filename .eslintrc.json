{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "linebreak-style": 0,
      "no-use-before-define": 0,
      "react/jsx-props-no-spreading": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types":0,
      "jsx-a11y/click-events-have-key-events":0
    }
}